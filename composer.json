{
    "name": "laravel-notification-channels/aws-sns",
    "description": "AWS SNS channel for Laravel Notifications",
    "homepage": "https://github.com/laravel-notification-channels/aws-sns",
    "license": "MIT",
    "authors": [
        {
            "name": "Menaf Gul",
            "email": "hi@menafgul.com",
            "homepage": "www.menafgul.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/notifications": "^5.3",
        "illuminate/support": "^5.1|^5.2|^5.3",
        "illuminate/queue": "^5.1|^5.2|^5.3",
        "aws/aws-sdk-php-laravel": "~3.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\AwsSns\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\AwsSns\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
